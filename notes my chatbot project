PRODIGY_HOME=scripts prodigy -F ./scripts/ner_custom.py ner.manual_hf test_dataset ./Data/combined_documents.txt --label PERSOON,ORGANISATIE,PROJECT,BEDRAG,LOCATIE,TIJDSPERIODE,SUBSIDIE

